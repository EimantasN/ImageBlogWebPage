@model ImageGalleryAndBlog.Models.TableModel

<body class="sticky-header left-side-collapsed">
    <section>

        @{
            Html.RenderPartial("_side_navigation.cshtml");
        }

        <!-- main content start-->
        <div class="main-content main-content2">
            <!-- header-starts -->
            <div class="header-section">

                <!--toggle button start-->
                <a class="toggle-btn  menu-collapsed">
                    <i class="fa fa-bars"></i>
                </a>
                <!--toggle button end-->

                @{
                    Html.RenderPartial("_top_navigation.cshtml");
                }
            </div>
            <!-- //header-ends -->

            <div id="page-wrapper">

                <div class="graphs">

                    <h3 class="blank1">Subscribers</h3>

                    <div class="xs tabls">

                        <div class="bs-example4" data-example-id="contextual-table">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Email</th>
                                    </tr>
                                </thead>
                                <tbody style="overflow: hidden">
                                    @{
                                        foreach (var subscribers in Model.Subscribers)
                                        {
                                            <tr>
                                                <td>@subscribers.Id</td>
                                                <td>@subscribers.Email</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <h3 class="blank1"></h3>
                    <h3 class="blank1">Images DATA</h3>

                    <div class="xs tabls">
                        @* IMAGES DATA *@
                        <div class="bs-example4" data-example-id="contextual-table">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Category</th>
                                        <th>Created Date</th>
                                        <th>Url</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    @{
                                        foreach (var image in Model.Images)
                                        {
                                            <tr class="" style="overflow: hidden">
                                                <td>@image.Id</td>
                                                <td>@image.Category</td>
                                                <td>@image.Created</td>
                                                <td>@image.Url</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <h3 class="blank1"></h3>
                    <h3 class="blank1">Article DATA</h3>

                    <div class="xs tabls">

                        <div class="bs-example4" data-example-id="contextual-table">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Title</th>
                                        <th>Author</th>
                                        <th>Text Length</th>
                                        <th>Created</th>
                                        <th>ImgUrl</th>
                                        <th>SubText Length</th>
                                        <th>Category</th>
                                        <th>Likes</th>
                                        <th>Dislikes</th>
                                    </tr>
                                </thead>
                                <tbody style="overflow: hidden">
                                    @{
                                        foreach (var arc in Model.Article)
                                        {
                                            <tr>
                                                <td>@arc.Id</td>
                                                <td>@arc.Title</td>
                                                <td>@arc.Author</td>
                                                <td>@arc.Text.Length</td>
                                                <td>@arc.Created</td>
                                                <td>@arc.ImgUrl</td>
                                                <td>@arc.SubText.Length</td>
                                                <td>@arc.Category</td>
                                                <td>@arc.Likes</td>
                                                <td>@arc.Dislikes</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <h3 class="blank1"></h3>
                    <h3 class="blank1">Article Comments DATA</h3>

                    <div class="xs tabls">

                        <div class="bs-example4" data-example-id="contextual-table">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Author</th>
                                        <th>Company</th>
                                        <th>Text Length</th>
                                        <th>Created</th>
                                        <th>Likes</th>
                                        <th>Dislikes</th>
                                    </tr>
                                </thead>
                                <tbody style="overflow: hidden">
                                    @{
                                        foreach (var com in Model.ArticleComment)
                                        {
                                            <tr>
                                                <td>@com.Id</td>
                                                <td>@com.Author</td>
                                                <td>@com.Company</td>
                                                <td>@com.Text.Length</td>
                                                <td>@com.Created</td>
                                                <td>@com.Likes</td>
                                                <td>@com.Dislikes</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</body>